page,.chat-content{
  position:relative;
  height:100%;
  background:#1e195d;
}
page .chat-bg{
  position:fixed;
  top:0;left:0;
  width:100%;height:100%;
  z-index:0;
}
page .container{
  position:relative;
  z-index:1;
}

.container{
  padding:7rpx 24rpx;
  height:calc(100% - 98rpx);
  
}
.container .connect{
  margin-bottom:10rpx;
  font-size:32rpx;
  line-height:40rpx;
  color:#11d1e1;
}
.container .connect view:first-child{
  margin-bottom:14rpx;
}

.container .welcome{
  margin-bottom:10rpx;
  line-height:40rpx;
  font-size:32rpx;
  color:#11d1e1;
}
.container .welcome .name{
  display:inline-block;
  color:#cae012;
  margin:0 12rpx;
}
.container .welcome .color{
  display:inline-block;
  color:#ff607e;
}

.container .chat-li{
  margin-bottom:16rpx;
  font-size:0;
}
.container .chat-li .chat-li-cont{
  display:inline-block;
  padding:8rpx 20rpx;
  border:2rpx solid #105795;
  background:rgba(21, 129, 204, .2);
  box-shadow:0px 0px 10px rgba(0, 116, 171, .3) inset;
  font-size:32rpx;
  color:#fff;
}

.container .myself .chat-li-cont{
  border-color:#f31e46;
  background:rgba(255, 16, 50, .2);
  box-shadow:none;
}

.container .chat-li .chat-li-cont .name{
  display:inline-block;
  line-height:46rpx;
  color:#cae012;
}

.chat-bottom{
  position:fixed;
  bottom:0;left:0;
  padding:14rpx;
  height:70rpx; 
  width:calc(100% - 28rpx);
  background:rgba(54, 95, 186, .5);
  z-index:4;
}

.chat-bottom .chat-input-area{
  height:70rpx;
  margin-bottom:16rpx;
  overflow:hidden;
}
.chat-bottom .chat-input-area .input-area{
  float:left;
  height:66rpx;
  line-height:66rpx;
  padding:0 18rpx;
  width:424rpx;
  border:2rpx solid #0ba1e9;
  background:rgba(7,46,135,.8);
  font-size:32rpx;color:#f4fbff;
}

.chat-bottom .chat-input-area .fast,
.chat-bottom .chat-input-area .send{
  float:right;
  width:110rpx;height:70rpx;
}

.chat-bottom .chat-input-area image{
  display:block;
  width:100%;height:100%;
}
.chat-bottom .chat-input-area .send{
  margin:0 4rpx 0 22rpx;
}
.chat-bottom .chat-input-area .disable{
  filter:grayscale(100%);
}

.chat-bottom .fast-ibox{
  display:none;
  padding:30rpx 24rpx;
  width:calc(100% - 48rpx);height:470rpx;
  background:rgba(0, 58, 136, .98);
}
.chat-bottom .fast-ibox li{
  display:block;
  margin:0 auto 20rpx;
  height:76rpx;
  line-height:76rpx;
  border:2rpx solid #0680bb;
  background:rgba(21, 129, 204, .2);
  box-shadow:0px 0px 10px rgba(0, 116, 171, .3) inset;
  font-size:32rpx;color:#d3e6f1;
  text-align:center;
}

.chat-bottom .message-title{
  position:absolute;
  right:36rpx;top:-80rpx;
  width:74rpx;height:89rpx;
  z-index:5;
}
.chat-bottom .message-title image{
  display:block;
  width:100%;height:100%;
}
.chat-bottom .message-title .num{
  position:absolute;
  top:0;left:0;
  width:100%;height:66rpx;
  line-height:66rpx;
  font-size:30rpx;color:#fff;
  text-align:center;
  z-index:1;
}

.active-fast{
  height:auto;
}
.active-fast .fast-ibox{
  display:block;
}

.program-btn{
  position:fixed;
  right:20rpx;bottom:340rpx;
  width:106rpx;height:160rpx;
  z-index:5;
}
.program-btn image,
.luck-btn image{
  display:block;
  width:100%;height:100%;
}

.luck-btn{
  position:fixed;
  right:20rpx;bottom:520rpx;
  width:106rpx;height:200rpx;
  z-index:5;
}

.program-ibox{
  /* display:none; */
  position:fixed;
  top:50%;left:50%;
  width:639rpx;height:1005rpx;
  margin:-502rpx 0 0 -320rpx;
  z-index:10;
}
.program-ibox image{
  display:block;
  width:100%;height:100%;
}
.program-ibox .close{
  position:absolute;
  top:0;right:0;
  width:50rpx;height:50rpx;
  z-index:2;
}

.mask{
  display:none;
  position:fixed;
  top:0;left:0;
  width:100%;height:100%;
  background:rgba(0, 0, 0, .5);
  z-index:8;
}

.luck-ibox{
  /* display:none; */
  position:fixed;
  top:50%;left:50%;
  margin:-65rpx 0 0 -260rpx;
  width:520rpx;height:210rpx;
  background:#fff;
  z-index:10;
}
.luck-ibox .luck-top{
  height:130rpx;
  line-height:130rpx;
  border-bottom:1rpx solid #b8b9bc;
  font-size:32rpx;color:#000;
  text-align:center;
}
.luck-ibox .luck-top .num{
  display:inline-block;
  color:#26b8f3;
}
.luck-ibox .luck-bottom{
  height:79rpx;line-height:79rpx;
  font-size:32rpx;color:#000;
  text-align:center;
}

.luck-ibox .luck-bottom .btn-1{
  float:left;
  width:50%;height:100%;
  border-right:1rpx solid #b8b9bc;
}
.luck-ibox .luck-bottom .btn-2{
  color:#26b8f3;
}

.winning-ibox{
  /* display:none; */
  position:fixed;
  top:50%;left:50%;
  margin:-300rpx 0 0 -250rpx;
  width:500rpx;height:600rpx;
  z-index:10;
}
.winning-ibox image{
  display:block;
  width:100%;height:100%;
}
.winning-ibox .close{
  position:absolute;
  bottom:0;left:0;
  width:100%;height:90rpx;
}

.guess-btn{
  position:fixed;
  z-index: 5;
  bottom:200rpx;right:20rpx;
  width:106rpx;height:120rpx;
}
.guess-btn image{
  display:block;
  width:100%;height:100%;
}

.guess-ibox{
  position:absolute;
  top:50%;left:50%;
  margin:-502rpx 0 0 -319rpx;
  width:639rpx;height:1005rpx;
  z-index:10;
}
.guess-ibox .close{
  position:absolute;
  top:0;right:0;
  width:80rpx;height:80rpx;
  z-index:2;
}

.guess-ibox .guess-top{
  position:relative;
  padding-top:107rpx;
  width:100%;height:217rpx;
  text-align:center;
}
.guess-ibox .guess-bottom{
  position:relative;
  width:100%;height:681rpx;
}

.guess-ibox .guess-top image,
.guess-ibox .guess-bottom image{
  position:absolute;
  top:0;left:0;
  width:100%;height:100%;
  z-index:0;
}

.guess-ibox .guess-top .nothing-text{
  display:none;
  position:relative;
  height:217rpx;line-height:217rpx;
  font-size:48rpx;color:#7ab1cf;
  text-align:center;
  z-index:1;
}
.guess-ibox .guess-bottom .nothing-text{
  display:none;
  position:relative;
  height:400rpx;line-height:400rpx;
  font-size:48rpx;color:#7ab1cf;
  text-align:center;
  z-index:1;
}

.guess-ibox .no-begin .nothing-text{
  display:block;
}

.guess-ibox .guess-bottom .guess-thead{
  position:relative;
  padding:16rpx 18rpx 0;
  height:50rpx;
  overflow:hidden;
  z-index:1;
}
.guess-ibox .guess-bottom .guess-thead li,
.guess-ibox .guess-bottom .guess-tbody .li{
  float:left;
  height:50rpx;line-height:50rpx;
  font-size:32rpx;color:#7ab1cf;
  text-align:center;
}
.guess-ibox .guess-bottom .guess-thead .li-1,
.guess-ibox .guess-bottom .guess-tbody .li-1{
  width:192rpx;
}
.guess-ibox .guess-bottom .guess-thead .li-2,
.guess-ibox .guess-bottom .guess-tbody .li-2{
  width:230rpx;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.guess-ibox .guess-bottom .guess-thead .li-3,
.guess-ibox .guess-bottom .guess-tbody .li-3{
  width:180rpx;
}

.guess-ibox .guess-top .choose{
  position:relative;
  display:inline-block;
  margin:44rpx 64rpx 0;
  width:130rpx;height:130rpx;
  line-height:130rpx;
  font-size:70rpx;color:#fff;
  z-index:1;
}
.guess-ibox .guess-top .choose .choose-1{
  background:rgba(255, 91, 126, .3);
  border-radius:100%;
}
.guess-ibox .guess-top .choose .choose-2{
  background:rgba(52, 146, 251, .3);
  border-radius:100%;
}

.guess-ibox .guess-top .active .choose-1{
  background:#e7395e;
}
.guess-ibox .guess-top .active .choose-2{
  background:#3492fb;
}

.guess-ibox .guess-bottom .guess-tbody{
  position:relative;
  padding:0 18rpx;
  z-index:1;
}
.guess-ibox .guess-bottom .tr{
  height:56rpx;line-height:56rpx;
}
.guess-ibox .guess-bottom .guess-tbody .li{
  height:56rpx;
  color:#fff;
  font-size:28rpx;
}

.guess-ibox .guess-bottom .guess-tbody .myself .li{
  margin-top:6rpx;
  color:#cae012;
}

.guess-ibox .no-begin .choose,
.guess-ibox .no-begin .guess-tbody .tr{
  display:none;
}

.guess-ibox-1{
  position:fixed;
  top:50%;left:50%;
  margin:-105rpx 0 0 -260rpx;
  width:520rpx;height: 210rpx;
  z-index:11;
}

.guess-ibox-1 .guess-bg-1{
  position:absolute;
  top:0;left:0;
  width:100%;height:100%;
  z-index:1;
}
.guess-ibox-1 .guess-top{
  position:relative;
  height:130rpx;line-height:130rpx;
  font-size:32rpx;color:#fff;
  text-align:center;
  z-index:2;
}
.guess-ibox-1 .guess-bottom{
  position:relative;
  height:80rpx;line-height:80rpx;
  font-size:32rpx;color:#fff;
  z-index:2;
}
.guess-ibox-1 .guess-bottom .btn-1,
.guess-ibox-1 .guess-bottom .btn-2{
  float:left;
  width:260rpx;
  text-align:center;
}
.guess-ibox-1 .guess-bottom .btn-2{
  color:#57a6ff;
}

.set-btn{
  position:fixed;
  z-index: 5;
  bottom:740rpx;right:20rpx;
  width:106rpx;height:140rpx;
}
.set-btn image{
  display:block;
  width:100%;height:100%;
}

.set-ibox{
  display:none;  
  position:absolute;
  top:50%;left:50%;
  margin:-514rpx 0 0 -308rpx;
  width:616rpx;height:1028rpx;
  border:3rpx solid #1168b1;
  background:rgba(3, 24, 67, .96);
  z-index:11;
}
.set-ibox .close{
  position:absolute;
  top:20rpx;right:20rpx;
  width:30rpx;height:30rpx;
}
.set-ibox .close image{
  display:block;
  width:100%;height:100%;
}

.set-ibox .set-title{
  padding-top:26rpx;
  height: 170rpx;line-height:170rpx;
  font-size:38rpx;color:#fff;
  text-align:center;
}
.set-ibox .set-btn-area{
  height:210rpx;
  margin-bottom:104rpx;
  text-align:center;
}
.set-ibox .set-btn-area .btn-bg{
  display:inline-block;
  width:210rpx;height:210rpx;
  border-radius:100%;
  background:rgba(100, 119, 148, .4);
  text-align:center;
}
.set-ibox .set-btn-area .btn-bg .set-ibox-btn{
  display:inline-block;
  width:170rpx;height:170rpx;
  line-height:170rpx;
  margin-top:20rpx;
  border-radius:100%;
  background:rgba(68, 101, 150, .72);
  font-size:40rpx;color:#fff;
}

.set-ibox .begin-guess .btn-bg{
  background:rgba(241, 0, 51, .4);
}
.set-ibox .begin-guess .btn-bg .set-ibox-btn{
  background:rgba(243, 54, 95, 1);
}

.set-ibox .mid-title{
  height:72rpx;line-height:72rpx;
  font-size:38rpx;color:#fff;
  text-align:center;
}

.set-ibox .set-choose{
  margin-bottom:44rpx;
  text-align:center;
}
.set-ibox .set-choose .choose{
  position:relative;
  display:inline-block;
  margin:40rpx 64rpx;
  width:130rpx;height:130rpx;
  line-height:130rpx;
  font-size:70rpx;color:#fff;
  z-index:1;
}
.set-ibox .set-choose .choose .choose-1{
  background:rgba(255, 91, 126, .3);
  border-radius:100%;
}
.set-ibox .set-choose .choose .choose-2{
  background:rgba(52, 146, 251, .3);
  border-radius:100%;
}

.set-ibox .set-choose .active .choose-1{
  background:#e7395e;
}
.set-ibox .set-choose .active .choose-2{
  background:#3492fb;
}

.set-ibox .set-real-btn{
  width:370rpx;height:86rpx;
  line-height:86rpx;
  margin:auto;
  border:2rpx solid #616da2;
  background:rgba(69, 103, 152, .3);
  font-size:38rpx;color:#6c90c7;
  text-align:center;
}
.set-ibox .active-set-btn{
  border-color:#3554d6;
  background:-webkit-linear-gradient(top, #052177, #1439bc);
  color:#fff;
}
